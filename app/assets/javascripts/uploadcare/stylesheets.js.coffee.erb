uploadcare.whenReady ->
  {jQuery: $} = uploadcare
  {tpl} = uploadcare.templates

  css = tpl('styles')

  style = document.createElement('style')
  style.setAttribute('type', 'text/css')
  if style.styleSheet?
    style.styleSheet.cssText = css
  else
    style.appendChild(document.createTextNode(css))
  $('head').append(style)
