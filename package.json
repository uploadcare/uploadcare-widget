{
  "name": "uploadcare-widget",
  "version": "3.7.9",
  "description": "Uploadcare Widget: file uploader.",
  "main": "uploadcare.js",
  "files": [
    "uploadcare.api.js",
    "uploadcare.api.min.js",
    "uploadcare.full.js",
