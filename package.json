{
  "name": "uploadcare-widget",
  "version": "0.0.0",
  "description": "Uploadcare widget source",
  "scripts": {
    "lint:css": "stylelint app/**/*.pcss",
    "lint:js": "eslint .",
    "build:svg": "svg-sprite --config svg-sprite.config.json app/assets/images/uploadcare/svg/icon-*.svg",
    "build:css": "postcss ./app/assets/stylesheets/uploadcare/styles.pcss -o ./app/assets/stylesheets/uploadcare/styles.css",
    "build:css:min": "cross-env NODE_ENV=production npm run build:css",
    "build": "npm run build:svg && npm run build:css:min",
    "test": "jest",
    "serve:latest": "browser-sync start --ss ./pkg/latest -f ./pkg/latest --port 3219"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-preset-env": "^1.5.2",
    "browser-sync": "^2.18.12",
    "cross-env": "^5.0.0",
    "cssnano": "^3.9.1",
    "eslint": "^3.19.0",
    "eslint-config-uploadcare": "^1.1.0",
    "jest": "^20.0.4",
    "jquery": "^3.2.1",
    "path": "^0.12.7",
    "postcss-calc": "^6.0.0",
    "postcss-cli": "^4.0.0",
    "postcss-color-function": "^4.0.0",
    "postcss-css-variables": "^0.7.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-import": "^10.0.0",
    "postcss-nested": "^2.0.2",
    "postcss-prefixer": "^1.0.4",
    "postcss-reporter": "^3.0.0",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-config-uploadcare": "^1.0.0",
    "svg-sprite": "^1.3.6"
  },
  "browserslist": [
    "> .4%",
    "ie >= 10"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uploadcare/uploadcare-widget.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uploadcare/uploadcare-widget/issues"
  },
  "homepage": "https://github.com/uploadcare/uploadcare-widget#readme"
}
