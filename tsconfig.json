{
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "lib": ["DOM", "es5"],
    "allowJs": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "outDir": "raw",
    "declaration": false,
    "rootDir": "src",
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "target": "ESNext"
  }
}
